{
  "name": "react-googlemaps-dynamic",
  "version": "0.4.0",
  "description": "Experimental fork of react-googlemaps that can dynamically load the Google Maps API",
  "keywords": [
    "react",
    "react-component",
    "google maps",
    "googlemaps",
    "maps"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build-examples": "webpack --devtool inline-source-map",
    "watch-examples": "webpack --devtool inline-source-map --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josh-/react-googlemaps-dynamic.git"
  },
  "homepage": "https://github.com/josh-/react-googlemaps-dynamic",
  "bugs": "https://github.com/josh-/react-googlemaps-dynamic/issues",
  "author": "Josh Parnham",
  "license": "MIT",
  "dependencies": {
    "react": "^0.13.3"
  },
  "devDependencies": {
    "jest-cli": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "react-tools": "^0.12.2",
    "webpack": "^1.3.7"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
